{
    "name": "bseries/base_media",
    "description": "Media functionality for Bento.",
    "type": "lithium-library",
    "license": ["BSD-3-Clause"],
    "require": {
        "php": "^5.5 || ^7.0",
        "bseries/base_core": "1.5.*@dev",
        "atelierdisko/cute": "0.3.*@dev",
        "davidpersson/mm": "2.2@dev || ^2.2",
        "davidpersson/temporary": "1.*@stable",
        "atelierdisko/li3_flash_message": "1.*",
        "atelierdisko/jsend": "1.0.*",
        "mpratt/embera": "1.8.*",
        "unionofrad/li3_behaviors": "2.*",
        "unionofrad/lithium": "^1.1",
        "composer/installers": "1.*@stable"
